                <div class="slds-form__item" role="listitem">
                    <div class="slds-form-element slds-form-element_stacked slds-is-editing no-abbr">
                        <lightning:combobox label="&nbsp;" 
                                            value="{!v.emp.Months__c}" 
                                            options="{! v.monthsOptions }" 
                                            placeholder="Months"
                                            onchange="{!c.changeMonthYear}"
                                            aura:id="fieldcheck"
                                            required="true"/>
                    </div>
                </div>
            </div>
            
        </aura:if>
        <aura:if isTrue="{!v.emp.FinServ__EmploymentStatus__c=='Self Employed / Contractor' || 
