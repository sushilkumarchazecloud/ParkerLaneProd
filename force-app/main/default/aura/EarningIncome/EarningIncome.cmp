                <div class="slds-form__item" role="listitem">
                    <div class="slds-form-element slds-form-element_stacked slds-is-editing">
                        <lightning:input label="Previous year net profit" formatter="currency"
                                         type="number" 
                                         value="{!v.emp.Previous_year_net_profit__c}"
                                         placeholder="Previous year net profit"
                                         aura:id="fieldcheck"
                                         required="true"/>
                    </div>
                </div>
            </div>
            <div class="slds-form__row">
                <div class="slds-form__item" role="listitem">
                    <div class="slds-form-element slds-form-element_stacked slds-is-editing">
                        <lightning:radioGroup class="slds-p-around_x-small" name="{!'salaryProfitRG'+ v.applicant.Id + v.index}"
                                              label="Do you pay yourself a salary on top of profit"    
                                              options="{! v.yesNoOptions }"
                                              value="{! v.emp.Pay_yourself_salary__c}"
