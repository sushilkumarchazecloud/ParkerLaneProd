                                    </aura:iteration>
                                </lightning:select>
                            </div>
                        </div>
                    </div>
                    <div class="slds-form__row slds-m-vertical_x-small">
                        <div class="slds-form__item" role="listitem">
                            <div class="slds-form-element slds-form-element_stacked slds-is-editing">
                                <lightning:input aura:id="fieldcheck" required="true"
                                                value="{!v.emp.Suburb__c}"
                                                label="Suburb" placeholder="Suburb"
                                                maxlength="100"
                                                class="no-abbr"/>
                            </div>
                        </div>
                        <div class="slds-form__item" role="listitem">
                            <div class="slds-form-element slds-form-element_stacked slds-is-editing no_animation">
                                <!--<lightning:combobox aura:id="fieldcheck" required="true"
                                                    label="State" 
