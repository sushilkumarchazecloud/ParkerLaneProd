                                                    label="&nbsp;" 
                                                    value="{!item.Child_Age__c}" 
                                                    options="{!v.ageYearsOptions}" 
                                                    placeholder="{!'Child '+(index+1)}"
                                                    class="no-abbr slds-scope"
                                                    dropdownAlignment="bottom-left"
                                                    onchange="{!c.changeDependent}"/>-->
                                
                                <lightning:select name="Child_Age" aura:id="fieldcheck"
                                                class="no-abbr slds-p-left_xx-small slds-m-left_xx-small"
                                                required="true"
                                                onchange="{!c.changeDependent}"
                                                value="{!item.Child_Age__c}"   
                                                label="">
                                    
                                    <option value="">Select</option>
                                    <aura:iteration items="{!v.ageYearsOptions}" var="option">
                                        <option value="{!option.value}" text="{!option.value}"></option>
                                    </aura:iteration>
                                </lightning:select>
                            </div>
                        </div>
                    </div>
                </aura:iteration>
                
            
        
