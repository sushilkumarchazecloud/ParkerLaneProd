                        <lightning:radioGroup name="{!'TimeThere' + v.index}"
                                            aura:id="fieldcheck"
                                            label="Time in this business" 
                                            value="{!v.emp.Years__c}" 
                                            onchange="{!c.changeMonthYear}"
                                            options="{!v.yearsOptionsList }"
                                            type="button"
                                            required="true"
                                            class="no-abbr "/> 
                    </div>
                </div>
            </aura:if>
            
            <aura:if isTrue="{!v.emp.EmploymentStatus__c == 'Pension' || v.emp.EmploymentStatus__c == 'Dividends or Annuity'
                            || v.emp.EmploymentStatus__c == 'Disability Pension' || v.emp.EmploymentStatus__c == 'Carer’s Pension'
                            || v.emp.EmploymentStatus__c == 'Child Support' || v.emp.EmploymentStatus__c == 'Other' || v.emp.EmploymentStatus__c == 'New Start Allowance'}"> 
                <div class="slds-grid slds-wrap">
                    <div class="slds-size_1-of-2">
                        <div class="input-dollar1 dollarField ">
                                <lightning:input label="Income amount" 
                                            value="{!v.emp.AnnualIncome__c}"
