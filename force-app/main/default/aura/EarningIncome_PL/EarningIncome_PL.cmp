                                                    options="{! v.statesTerritoriesOptions }" 
                                                    placeholder="State"
                                                    class="slds-scope no-abbr" 
                                                    dropdownAlignment="bottom-left"/>-->
                                
                                <lightning:select name="State" aura:id="fieldcheck"
                                                class="no-abbr" required="true"
                                                value="{!v.emp.State__c}"
                                                label="State">
                                    
                                    <option value="">Select</option>
                                    <aura:iteration items="{!v.statesTerritoriesOptions}" var="option">
                                        <option value="{!option.value}" text="{!option.value}"></option>
                                    </aura:iteration>
                                </lightning:select>
                            </div>
                        </div>
                    </div>
                    <div class="slds-form__row">
                        <div class="slds-form__item" role="listitem">
                            <div class="slds-form-element slds-form-element_stacked slds-is-editing">
                                <lightning:input aura:id="fieldcheck" required="true"
                                                value="{!v.emp.Postal_Code__c}" maxlength="10"
                                                label="Postal Code" placeholder="Postal Code"
                                                class="no-abbr"/>
                            </div>
                        </div>
                        <div class="slds-form__item" role="listitem">
                            <div class="slds-form-element slds-form-element_stacked slds-is-editing">
