                                                    aura:id="fieldcheck"
                                                    required="true"
                                                    class="slds-scope  slds-p-left_xx-small slds-m-left_xx-small no-abbr"
                                                    dropdownAlignment="bottom-left"/>-->
                        
                        <lightning:select name="Frequency" aura:id="fieldcheck"
                                        class="no-abbr slds-p-left_xx-small slds-m-left_xx-small"
                                        required="true"
                                        value="{!v.emp.Income_Frequency__c}"  
