                                                    dropdownAlignment="bottom-left"/> -->
                        
                        
                        <lightning:select name="Frequency" aura:id="fieldcheck"
                                        class="no-abbr slds-p-left_xx-small slds-m-left_xx-small"
                                        required="true"
                                        value="{!v.emp.Income_Frequency__c}"  
                                        label="Frequency">
                            
                            <option value="">Select</option>
                            <aura:iteration items="{!v.incomeFrequencyOptions}" var="option">
                                <option value="{!option.value}" text="{!option.value}"></option>
                            </aura:iteration>
                        </lightning:select>
                    </div>
