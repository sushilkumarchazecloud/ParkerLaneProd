                                            class=" no-abbr" 
                                            aura:id="fieldcheck"
                                            required="true"/>
                        </div>
                    </div>
                    <div class="slds-size_1-of-2" >
                        <div class="input-dollar1 dollarField slds-m-left_x-small ">
                            <lightning:input label="Previous year's income" 
                                            type="number" 
                                            value="{!v.emp.Previous_year_net_profit__c}"
                                            class=" no-abbr"
                                            aura:id="fieldcheck"
                                            required="true"/>
                        </div>
                    </div>
                </div>
