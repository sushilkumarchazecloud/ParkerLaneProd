    <aura:attribute name="titleOptions" type="List" default="[{'label': 'Mr.', 'value': 'Mr'},{'label': 'Mrs.', 'value': 'Mrs'},{'label': 'Ms.', 'value': 'Ms'},{'label': 'Dr.', 'value': 'Dr'}]"/>
    <aura:attribute name="cardReferenceOptions" type="List" default="[{'label': '1', 'value': '1'},{'label': '2', 'value': '2'},{'label': '3', 'value': '3'},{'label': '4', 'value': '4'},{'label': '5', 'value': '5'},{'label': '6', 'value': '6'},{'label': '7', 'value': '7'},{'label': '8', 'value': '8'},{'label': '9', 'value': '9'}]"/>
    <aura:attribute name="cardColourOptions" type="List" default="[{'label': 'Green', 'value': 'Green'},{'label': 'Blue', 'value': 'Blue'},{'label': 'Yellow', 'value': 'Yellow'}]"/>
    
    <aura:attribute name="maritalStatusOptions" type="List" default="[]"/>
    <aura:attribute name="CountriesOptions" type="List" default="[{'label': 'Australian', 'value': 'Australian'}]"/>
    <aura:attribute name="noChildrenOptions" type="List" default="[{'label': '0', 'value': '0'},{'label': '1', 'value': '1'},{'label': '2', 'value': '2'},{'label': '3', 'value': '3'},{'label': '4', 'value': '4'},{'label': '5+', 'value': '5'}]"/>

    <aura:attribute name="statesTerritoriesOptions" type="List" default="[{'label': 'Queensland', 'value': 'Queensland'},{'label': 'New South Wales', 'value': 'New South Wales'},{'label': 'Australian Capital Territory', 'value': 'Australian Capital Territory'},{'label': 'Victoria', 'value': 'Victoria'},{'label': 'South Australia', 'value': 'South Australia'},{'label': 'Western Australia', 'value': 'Western Australia'},{'label': 'Tasmania', 'value': 'Tasmania'},{'label': 'Northern Territory', 'value': 'Northern Territory'}]"/>
    <aura:attribute name="monthsOptions" type="List" default="[{'label': '0', 'value': '0'},{'label': '1', 'value': '1'},{'label': '2', 'value': '2'},{'label': '3', 'value': '3'},{'label': '4', 'value': '4'},{'label': '5', 'value': '5'},{'label': '6', 'value': '6'},{'label': '7', 'value': '7'},{'label': '8', 'value': '8'},{'label': '9', 'value': '9'},{'label': '10', 'value': '10'},{'label': '11', 'value': '11'}]"/>
    <aura:attribute name="yearsOptions" type="List" default="[{'label': '0', 'value': '0'},{'label': '1', 'value': '1'},{'label': '2', 'value': '2'},{'label': '3', 'value': '3'},{'label': '4', 'value': '4'},{'label': '5', 'value': '5'},{'label': '6', 'value': '6'},{'label': '7', 'value': '7'},{'label': '8', 'value': '8'},{'label': '9', 'value': '9'},{'label': '10+', 'value': '10+'}]"/>
    <aura:attribute name="ageYearsOptions" type="List" default="[{'label': '0 Year', 'value': '0 Year'},{'label': '1 Year', 'value': '1 Year'},{'label': '2 Years', 'value': '2 Years'},{'label': '3 Years', 'value': '3 Years'},{'label': '4 Years', 'value': '4 Years'},{'label': '5 Years', 'value': '5 Years'},{'label': '6 Years', 'value': '6 Years'},{'label': '7 Years', 'value': '7 Years'},{'label': '8 Years', 'value': '8 Years'},{'label': '9 Years', 'value': '9 Years'},{'label': '10 Years', 'value': '10 Years'},{'label': '11 Years', 'value': '11 Years'},{'label': '12 Years', 'value': '12 Years'},{'label': '13 Years', 'value': '13 Years'},{'label': '14 Years', 'value': '14 Years'},{'label': '15 Years', 'value': '15 Years'},{'label': '16 Years', 'value': '16 Years'},{'label': '17 Years', 'value': '17 Years'},{'label': '18 Years', 'value': '18 Years'}]"/>
    <aura:attribute name="streetTypeOptions" type="List" default="[{'label': 'Alley', 'value': 'Alley'},{'label': 'Ambl', 'value': 'Ambl'},{'label': 'Arcade', 'value': 'Arcade'},{'label': 'Avenue', 'value': 'Avenue'},{'label': 'Bend', 'value': 'Bend'},{'label': 'Boulevard', 'value': 'Boulevard'},{'label': 'Brace', 'value': 'Brace'},{'label': 'Centre', 'value': 'Centre'},{'label': 'Chase', 'value': 'Chase'},{'label': 'Circle', 'value': 'Circle'},{'label': 'Circuit', 'value': 'Circuit'},{'label': 'Circus', 'value': 'Circus'},{'label': 'Close', 'value': 'Close'},{'label': 'Concourse', 'value': 'Concourse'},{'label': 'Corner', 'value': 'Corner'},{'label': 'Corso', 'value': 'Corso'},{'label': 'Court', 'value': 'Court'},{'label': 'Crescent', 'value': 'Crescent'},{'label': 'Crossing', 'value': 'Crossing'},{'label': 'Drive', 'value': 'Drive'},{'label': 'Entrance', 'value': 'Entrance'},{'label': 'Esplanade', 'value': 'Esplanade'},{'label': 'Freeway', 'value': 'Freeway'},{'label': 'Frontage', 'value': 'Frontage'},{'label': 'Gate', 'value': 'Gate'},{'label': 'Glade', 'value': 'Glade'},{'label': 'Glen', 'value': 'Glen'},{'label': 'Grange', 'value': 'Grange'},{'label': 'Green', 'value': 'Green'},{'label': 'Ground', 'value': 'Ground'},{'label': 'Grove', 'value': 'Grove'},{'label': 'Heights', 'value': 'Heights'},{'label': 'Highway', 'value': 'Highway'},{'label': 'Junction', 'value': 'Junction'},{'label': 'Lane', 'value': 'Lane'},{'label': 'Link', 'value': 'Link'},{'label': 'Loop', 'value': 'Loop'},{'label': 'Mews', 'value': 'Mews'},{'label': 'Mount', 'value': 'Mount'},{'label': 'Nook', 'value': 'Nook'},{'label': 'Outlook', 'value': 'Outlook'},{'label': 'Parade', 'value': 'Parade'},{'label': 'Parkway', 'value': 'Parkway'},{'label': 'Pass', 'value': 'Pass'},{'label': 'Place', 'value': 'Place'},{'label': 'Plaza', 'value': 'Plaza'},{'label': 'Point', 'value': 'Point'},{'label': 'Port', 'value': 'Port'},{'label': 'Promenade', 'value': 'Promenade'},{'label': 'Quadrangle', 'value': 'Quadrangle'},{'label': 'Quadrant', 'value': 'Quadrant'},{'label': 'Quay', 'value': 'Quay'},{'label': 'Reserve', 'value': 'Reserve'},{'label': 'Rest', 'value': 'Rest'},{'label': 'Retreat', 'value': 'Retreat'},{'label': 'Ridge', 'value': 'Ridge'},{'label': 'Right of Way', 'value': 'Right of Way'},{'label': 'Rise', 'value': 'Rise'},{'label': 'Road', 'value': 'Road'},{'label': 'Roadway', 'value': 'Roadway'},{'label': 'Round', 'value': 'Round'},{'label': 'Row', 'value': 'Row'},{'label': 'Siding', 'value': 'Siding'},{'label': 'Square', 'value': 'Square'},{'label': 'Street', 'value': 'Street'},{'label': 'Terrace', 'value': 'Terrace'},{'label': 'Tollway', 'value': 'Tollway'},{'label': 'Track', 'value': 'Track'},{'label': 'View', 'value': 'View'},{'label': 'Walk', 'value': 'Walk'},{'label': 'Walkway', 'value': 'Walkway'},{'label': 'Way', 'value': 'Way'},{'label': 'Wynd', 'value': 'Wynd'}]"/>
    
    <aura:attribute name="citizenshipStatus" type="String" default=""/>
    <aura:attribute name="showPreviousAdd" type="Boolean" default="false"/>
    <aura:attribute name="addressList" type="String[]" />
    <aura:attribute name="homeAssetList" type="FinServ__AssetsAndLiabilities__c[]"/>
    
    
    
    <aura:attribute name="propertysList" type="FinServ__AssetsAndLiabilities__c[]"/>
    
    <aura:attribute name="otherProperty" type="String" default="No"/>
    <aura:attribute name="livingSituation" type="String" default="No"/>
    <aura:attribute name="otherPropertyOption" type="List" default="[{'label': 'Own outright', 'value': 'Own outright'},{'label': 'Own with mortgage', 'value': 'Own with mortgage'}]"/>
    
    <aura:attribute name="livingSituationOptionsList" type="List" default="[{'label': 'Yes - own outright', 'value': 'Own home outright'},{'label': 'Yes - own with mortgage', 'value': 'Own home with mortgage'},{'label': 'No - Renting / boarding', 'value': 'Renting'}]"/>
    
    <!-- Own And Owe-->
    <aura:attribute name="isNotAsset" type="Boolean" default="false"/>
    <aura:attribute name="isNotLiability" type="Boolean" default="false"/>
    <aura:attribute name="assetsList" type="FinServ__AssetsAndLiabilities__c[]" />
    <aura:attribute name="allAssetsList" type="FinServ__AssetsAndLiabilities__c[]" />
    <aura:attribute name="homeAsset" type="FinServ__AssetsAndLiabilities__c" default="{'sobjectType': 'FinServ__AssetsAndLiabilities__c',
            																			'FinServ__AssetsAndLiabilitiesType__c':''}"/>
    <aura:attribute name="assetsSelected" type="FinServ__AssetsAndLiabilities__c"/>
    <aura:attribute name="liabilitiesSelected" type="FinServ__AssetsAndLiabilities__c" />
    <aura:attribute name="totalAssetsAmount" type="Decimal" default="0"/>
    <aura:attribute name="totalLiabilitiesAmount" type="Decimal" default="0"/>
    <aura:attribute name="selectedType" type="String" default=""/>
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="assetTypeOptions" type="List" default="[{'label': 'Add another asset', 'value': ''},{'label': 'Investment Property', 'value': 'Investment Property'},{'label': 'Commercial Property', 'value': 'Commercial Property'},{'label': 'Shares Portfolio', 'value': 'Shares Portfolio'}]"/>
    <aura:attribute name="addAssetValue" type="String" default=""/>
    
    <aura:attribute name="opportunity" type="Opportunity" />
    <!-- Own And Owe-->
    <aura:attribute name="assetsSelectedYesorNO" type="FinServ__AssetsAndLiabilities__c" default="[{'sobjectType': 'FinServ__AssetsAndLiabilities__c'}]"/>
    
    
    <aura:attribute name="incomeFrequencyOptions" type="List" default="[{'label': 'Weekly', 'value': 'Weekly'},{'label': 'Fortnightly', 'value': 'Fortnightly'},{'label': 'Monthly', 'value': 'Monthly'},{'label': 'Annually', 'value': 'Annually'}]"/>
    <aura:attribute name="OwnershipOptions" type="List" default="[{'label': 'Applicant 1', 'value': 'Applicant 1'},{'label': 'Applicant 2', 'value': 'Applicant 2'},{'label': 'Joint', 'value': 'Joint'},{'label': 'Joint (non-applicant)', 'value': 'Joint (non-applicant)'}]"/>
   
    <aura:attribute name="isOpenAlertModal" type="Boolean" default="false"/>
    <aura:attribute name="deleteIndex" type="Integer" default="0"/>
    
    <aura:attribute name="OtherAsset" type="FinServ__AssetsAndLiabilities__c" default="{'sobjectType': 'FinServ__AssetsAndLiabilities__c',
                                                                                        'FinServ__AssetsAndLiabilitiesType__c':''}"/>
                                                                                        
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <aura:handler name="monthYearChangeEvent" event="c:monthYearChangeEvent" action="{!c.changeMonthYear}"/>
    <aura:handler name="SolarDeleteRowEvent" event="c:SolarDeleteRowEvent" action="{!c.deleteRow}"/>
    <aura:method name="checkAboutDetails" action="{!c.checkAboutDetails}" access="PUBLIC" />
    
    
    <div class="slds-clearfix slds-form max-700">
        
        <div class="slds-form" role="list">
            <div class="slds-form-element slds-form-element_stacked slds-is-editing slds-form-element_1-col rg-split margin_set">
                
                <div class="slds-form" role="list">
                    <c:ApplicantDetailAddress_PL aura:id="appDetailAddress"
                                              headerLabel = "Your home address"
                                              applicantNo = "{!v.applicantNo}"
                                              years = "{!v.applicant.Year__c}"
                                              months = "{!v.applicant.Months__c}"
                                              isFindAdd ="{!v.applicant.Is_Find_Address__c}"
                                              searchString ="{!v.applicant.Residential_Address__c}"
                                              unitNumber = "{!v.applicant.Unit_Number__c}"
                                              streetNumber = "{!v.applicant.Street_Number__c}"
                                              street = "{!v.applicant.Street__c}"
                                              streetType = "{!v.applicant.Street_Type__c}"
                                              suburb = "{!v.applicant.Suburb__c}"
                                              state = "{!v.applicant.State__c}"
                                              postalCode = "{!v.applicant.Postal_Code__c}"
                                              country = "{!v.applicant.FinServ__CountryOfResidence__c}"/>

                    <aura:if isTrue="{!!v.applicant.Is_Previous_Address_Same__c}">
                        <aura:iteration items="{!v.addressList}" var="item" indexVar="index">
                            <aura:if isTrue = "{!index == 0}">
                                <div style="margin-top:20px;">
                                    <c:ApplicantDetailAddress_PL aura:id="appDetailAddress1"
                                                              headerLabel = "Previous Address"
                                                              applicantNo = "{!v.applicantNo}"
                                                              index = "{!index}"
                                                              years = "{!v.applicant.Year_1__c}"
                                                              months = "{!v.applicant.Months_1__c}"
                                                              isFindAdd ="{!v.applicant.Is_Find_Address_1__c}"
                                                              searchString ="{!v.applicant.Residential_Address_1__c}"
                                                              unitNumber = "{!v.applicant.Unit_Number_1__c}"
                                                              streetNumber = "{!v.applicant.Street_Number_1__c}"
                                                              street = "{!v.applicant.Street_1__c}"
                                                              streetType = "{!v.applicant.Street_Type_1__c}"
                                                              suburb = "{!v.applicant.Suburb_1__c}"
                                                              state = "{!v.applicant.State_1__c}"
                                                              postalCode = "{!v.applicant.Postal_Code_1__c}"
                                                              country = "{!v.applicant.Country_1__c}"/>
                                </div>
                                
                            </aura:if>
                        </aura:iteration>
                        
                        <aura:if isTrue = "{!v.showPreviousAdd &amp;&amp; v.applicant.Year__c !=NULL}">
                            <div class="slds-p-top_small slds-m-bottom_small">
                                <div class="slds-text-color_error slds-text-align_center">You must provide at least 3 years of Residential history. Please add another</div>
                                <lightning:button label="Add previous address"
                                                  onclick="{! c.addAddress }" 
                                                  iconName="utility:add" 
                                                  iconPosition="right"                                   
                                                  variant="brand-outline" 
                                                  class="slds-align_absolute-center addmore-btn slds-m-top_large"/>
                            </div>
                        </aura:if>
                    </aura:if>

                        <div class="slds-m-top_large" aura:id="changeIt"> 
                            <div class="slds-form__row">
                                <div class="slds-form__item" role="listitem">
                                    <div class="slds-form-element slds-form-element_stacked slds-is-editing slds-form-element_1-col">
                                        
                                        <lightning:select name="Relationship" aura:id="fieldcheck"
                                                          label="# of dependent children in the household?"
                                                          class="no-abbr" required="true"
                                                          value="{!v.applicant.FinServ__NumberOfChildren__c}"  onchange = "{!c.changeDependent}">
                                            
                                            <option value="">Select</option>
                                            <aura:iteration items="{!v.noChildrenOptions}" var="option">
                                                <option value="{!option.value}" text="{!option.value}"></option>
                                            </aura:iteration>
                                        </lightning:select>
                                    </div>
                                </div>
                            </div>
                        </div>

                    <div class="WhereYourlive slds-m-vertical_x-large">
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_10-of-12">
                                <h6 class="heading-edit">
                                    Do you own where you live?
                                </h6>
                            </div>
                        </div>    
                       
                       <!-- <aura:if isTrue="{!v.applicant.Do_you_own_where_you_live__c == 'Yes'}">-->
                            <div class="highlightOnselect ">
                                <div class="slds-grid slds-wrap"> 
                                    <div class="slds-size_2-of-2">                     
                                        <lightning:select aura:id="fieldcheck" name="select"  label=""
                                                          required="true" class="no-abbr "
                                                          value="{!v.applicant.Living_Situation__c}">
                                            <option value="">Select</option>
                                            <aura:iteration items="{!v.livingSituationOptionsList}" var="livingSituation">
                                                <option value="{!livingSituation.value}" text="{!livingSituation.label}"></option>
                                            </aura:iteration>
                                            
                                        </lightning:select>
                                    </div>
                                    
                                   <aura:if isTrue="{!v.applicant.Living_Situation__c == 'Renting'}">
                                        <div class="slds-size_1-of-2">
                                            <div class="input-dollar2 edit_inputbox">
                                                <lightning:input aura:id="fieldcheck" required="true"
                                                                 label="Rent" 
                                                                 type="number"
                                                                 value="{!v.applicant.Rental_Amount__c}"  
                                                                 class="no-abbr slds-m-right_x-small slds-p-right_xx-small slds-m-top_small"
                                                                 />
                                            </div>
                                        </div>
                                        <div class="slds-size_1-of-2">
                                            <lightning:select aura:id="fieldcheck" name="select"  label="Rental Frequency"
                                                              required="true" class="no-abbr slds-m-left_x-small slds-p-left_xx-small slds-m-top_small"
                                                              value="{!v.applicant.Rental_Frequency__c}">
                                                <option value="">Select</option>
                                                <aura:iteration items="{!v.frequencyOptions}" var="Frequency">
                                                    <option value="{!Frequency.value}" text="{!Frequency.value}"></option>
                                                </aura:iteration>
                                                
                                            </lightning:select>
                                        </div>
                                    </aura:if>
                                   <aura:if isTrue="{!v.applicant.Living_Situation__c == 'Own home outright'  || v.applicant.Living_Situation__c == 'Own home with mortgage'}">
                                        <div class="slds-size_1-of-2">
                                            <div class="input-dollar2 edit_inputbox">
                                                <lightning:input aura:id="fieldcheck" required="true"
                                                                 label="Value of home" 
                                                                 type="number"
                                                                 onchange="{!c.doCalculation}"
                                                                 value="{!v.homeAsset.Value__c}"  
                                                                 class="no-abbr slds-m-right_x-small slds-p-right_xx-small slds-m-top_small"
                                                                 />
                                            </div>
                                        </div>
                                        <div class="slds-size_1-of-2">
                                            <lightning:select aura:id="fieldcheck" name="select"  label="Ownership"
                                                              required="true" class="no-abbr slds-m-left_x-small slds-p-left_xx-small slds-m-top_small"
                                                              value="{!v.homeAsset.FinServ__Ownership__c}">
                                                <option value="">Select</option>
                                                <aura:iteration items="{!v.OwnershipOptions}" var="Ownership">
                                                    <option value="{!Ownership.value}" text="{!Ownership.value}"></option>
                                                </aura:iteration>
                                                
                                            </lightning:select>
                                        </div>
                                    </aura:if>
                                     
                                </div>
                                
                            </div>
                        <!-- </aura:if>-->
                    </div>
                    <div class="OtherProperty slds-m-top_medium"> 
                        <div class="slds-grid slds-wrap">
                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_10-of-12">
                                <h6 class="heading-edit">
                                    Do you own any other property?
                                </h6>
                            </div>
                            <div class="slds-col slds-size_1-of-1 slds-medium-size_6-of-12 slds-large-size_2-of-12 rg-split">
                                <lightning:radioGroup name="otherPropertyes"
                                                      aura:id="fieldcheck" 
                                                      label=""
                                                      options="{!v.yesNoOptions }"
                                                      value="{!v.applicant.Do_you_own_any_other_property__c}"
                                                      type="button"
                                                      onchange="{!c.addPropertyOnYes}"
                                                      required="true"
                                                      class="no-abbr"/>
                            </div>
                        </div>
                        
                        <aura:if isTrue="{!v.applicant.Do_you_own_any_other_property__c == 'Yes'}">
                            <aura:iteration items="{!v.propertysList}" var="property" indexVar="index">   
                                <div class="slds-grid slds-wrap">
                                    <div class="slds-size_1-of-2 slds-m-top_medium">
                                        <h6 class="subheading-edit"> Other Property {!index+1}</h6>
                                    </div>
                                    <aura:if isTrue ="{!index gt 0}">
                                        <div class="slds-size_1-of-2">
                                            <div class="slds-text-link_reset" id="{!index}" onclick="{!c.deleteProperty}">
                                                <lightning:icon iconName="utility:delete" size="x-small" style="float:right;margin-top:15px !important" class="slds-m-around_xx-small slds-p-around_xx-small"/>
                                            </div> 
                                        </div>
                                    </aura:if>
                                </div>
                                
                                <div class="highlightOnselect1">  
                                    <div class="slds-grid slds-wrap">
                                        <div class="slds-size_1-of-2 rg-split">
                                            <lightning:radioGroup name="{!'otherProperty'+index}"
                                                                  aura:id="fieldcheck" 
                                                                  label=""
                                                                  value="{!property.Property_Situation__c}"
                                                                  options="{!v.otherPropertyOption}"
                                                                  required="true"
                                                                  type="button"
                                                                  class="no-abbr"
                                                                  /> 
                                        </div>
                                        <div class="slds-size_2-of-2 slds-m-top_small">
                                            <c:AddressAPItool aura:id="addressTool" isFindAdd = "{!property.Is_Find_Address__c}"
                                                              searchString="{!property.Address__c}" label="Property Address"
                                                              streetNumber="{!property.Street_Number__c}" street="{!property.Street__c}"
                                                              state="{!property.State__c}" suburb="{!property.Suburb__c}"
                                                              postalCode="{!property.Postal_Code__c}" country="{!property.Country__c}"
                                                              streetType ="{!property.Street_Type__c}" /> 
                                        </div>
                                        <div class="slds-size_2-of-2">
                                            <div class="slds-form__row">
                                                <div class="slds-form__item" role="listitem">
                                                    <div class="slds-form-element slds-form-element_stacked slds-is-editing slds-form-element_1-col">
                                                        <lightning:input type="checkbox" 
                                                                         checked="{!property.Is_Find_Address__c}" 
                                                                         label=" Can't find your address?"/>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <aura:if isTrue="{!property.Is_Find_Address__c}">
                                            <div class="slds-size_1-of-2" >
                                                <lightning:input aura:id="fieldcheck" value="{!property.Unit_Number__c}"
                                                                 label="Unit Number (Optional)" placeholder="Unit Number"
                                                                 maxlength="50" class="slds-scope slds-m-top_xx-small slds-m-right_x-small slds-p-right_xx-small no-abbr"/>
                                                
                                            </div>
                                            <div class="slds-size_1-of-2" >
                                                <lightning:input aura:id="fieldcheck" value="{!property.Street_Number__c}" required="true"
                                                                 label="Street Number" placeholder="Street Number"
                                                                 maxlength="25" class="slds-scope slds-m-top_xx-small slds-m-le_x-small slds-p-left_xx-small no-abbr"/>
                                            </div>      
                                            <div class="slds-size_1-of-2">
                                                <lightning:input aura:id="fieldcheck" required="true"
                                                                 value="{!property.Street__c}"
                                                                 label="Street" placeholder="Street"
                                                                 maxlength="100"
                                                                 class="slds-scope slds-m-top_xx-small slds-p-right_xx-small slds-m-right_x-small no-abbr"/>
                                            </div>
                                            <div class="slds-size_1-of-2">
                                                <lightning:combobox aura:id="fieldcheck" required="true"
                                                                    label="Street Type" 
                                                                    value="{!property.Street_Type__c}" 
                                                                    options="{!v.streetTypeOptions}" 
                                                                    placeholder="Street Type"
                                                                    dropdownAlignment="bottom-left"
                                                                    class="slds-scope slds-m-top_xx-small slds-p-left_xx-small no-abbr slds-m-left_x-small"/>
                                            </div>
                                            <div class="slds-size_1-of-2">
                                                <lightning:input aura:id="fieldcheck" required="true"
                                                                 value="{!property.Suburb__c}"
                                                                 label="Suburb" placeholder="Suburb"
                                                                 maxlength="100"
                                                                 class="slds-scope slds-m-top_xx-small slds-p-right_xx-small slds-m-right_x-small no-abbr"/>
                                            </div>
                                            <div class="slds-size_1-of-2 ">
                                                <lightning:combobox aura:id="fieldcheck" required="true"
                                                                    label="State" 
                                                                    value="{!property.State__c}"
                                                                    options="{! v.statesTerritoriesOptions }" 
                                                                    placeholder="State"
                                                                    dropdownAlignment="bottom-left"
                                                                    class="slds-scope slds-m-top_xx-small slds-p-left_xx-small no-abbr slds-m-left_x-small no-abbr"/>
                                            </div>
                                            <div class="slds-size_1-of-2 ">
                                                <lightning:input aura:id="fieldcheck" required="true" maxlength="10"
                                                                 value="{!property.Postal_Code__c}"
                                                                 label="Postal Code" placeholder="Postal Code"
                                                                 class="slds-scope slds-m-top_xx-small slds-p-right_xx-small slds-m-right_x-small no-abbr"/>
                                            </div>
                                            <div class="slds-size_1-of-2 ">
                                                <lightning:input aura:id="fieldcheck" required="true"
                                                                 label="Country" maxlength="50"
                                                                 value="{!property.Country__c}" 
                                                                 placeholder="Country"
                                                                 class="slds-scope slds-m-top_xx-small slds-p-left_xx-small no-abbr slds-m-left_x-small no-abbr"/>
                                            </div>                                   
                                        </aura:if>
                                        <div class="slds-size_1-of-2 ">
                                            <div class="input-dollar2 edit_inputbox">
                                                <lightning:input label="Value" 
                                                                 type="number"
                                                                 required="true"
                                                                 aura:id="fieldcheck"
                                                                 value="{!property.Value__c}"
                                                                 class="no-abbr slds-m-top_small slds-p-right_xx-small slds-m-right_small"
                                                                 onchange="{!c.doCalculation}"/>   
                                            </div>
                                        </div>
                                        <div class="slds-size_1-of-2 ">
                                            
                                            <lightning:select aura:id="fieldcheck" name="select" onchange="" label="Ownership Share"
                                                              required="true" class="no-abbr slds-m-top_small slds-p-left_xx-small slds-m-left_small"
                                                              value="{!property.FinServ__Ownership__c}"
                                                              >
                                                <option value="">Select</option>
                                                <aura:iteration items="{!v.OwnershipOptions}" var="ownershipOption">
                                                    <option value="{!ownershipOption.value}" text="{!ownershipOption.value}"></option>
                                                </aura:iteration>
                                            </lightning:select>
                                        </div>
                                        
                                        <div class="slds-size_1-of-2 ">
                                            <div class="input-dollar2 edit_inputbox">
                                                <lightning:input label="Rental Income" 
                                                                 type="number" aura:id="fieldcheck" required="true"
                                                                 value="{!property.Rental_Amount__c}"
                                                                 class="no-abbr slds-m-top_small slds-p-right_xx-small slds-m-right_small"/>  
                                            </div>
                                        </div>
                                        <div class="slds-size_1-of-2 ">
                                            <lightning:select aura:id="fieldcheck" name="select" onchange="" label="Rental Frequency"
                                                              required="true" class="no-abbr slds-m-top_small slds-p-left_xx-small slds-m-left_small" 
                                                              value="{!property.Rental_Frequency__c}">
                                                <option value="">Select</option>
                                                <aura:iteration items="{!v.incomeFrequencyOptions}" var="incomeFrequency">
                                                    <option value="{!incomeFrequency.value}" text="{!incomeFrequency.value}"></option>
                                                </aura:iteration>
                                            </lightning:select>
                                        </div>
                                    </div>
                                </div>
                            </aura:iteration>
                            
                            <div class="slds-size_2-of-2 button_icon_radius">
                                <div class="slds-p-vertical_small text_decoration_underline">
                                    <lightning:button label="Add another property"
                                                      onclick="{!c.addMoreProperty}" 
                                                      iconName="utility:add" 
                                                      iconPosition="left"                                   
                                                      variant="base"
                                                      class="slds-align_absolute-center"/>
                                </div>
                            </div>
                            
                        </aura:if>
                    </div>
                </div>
            </div>
        </div>
        
    </div>
    
    <lightning:spinner aura:id="mySpinner" variant="brand" size="large" class="" />
    <section class="max-700 slds-clearfix slds-form ">
        <div class="slds-form slds-m-top--medium slds-p-left_xx-small" role="list">
            <h1 class = "header-text">Total value of your other assets?</h1>
        </div>
        <aura:if isTrue="{!v.opportunity.Lender__c == 'Transport Mutual Credit Union'}">  
            
            <div class="LenderTMCU" style="padding-left:5px;">
                <div class="slds-grid slds-wrap "> 
                    <div class="slds-size_2-of-2 ">                     
                        <h2 class="">Including superannuation, motor vehicles, home contents, shares and cash</h2>
                    </div>
                    <div class="slds-size_1-of-2 ">
                        <div class="input-dollar3 edit_inputbox">
                            <lightning:input aura:id="fieldcheck" required="true"
                                             label="" 
                                             type="number"
                                             value="{!v.OtherAsset.Value__c}"  
                                             class="no-abbr"
                                             onchange="{!c.doCalculation}"
                                             />
                        </div>
                    </div>
                </div>                      
            </div>   
            <aura:set attribute="else">
                <div class="">
                    <aura:iteration items="{!v.assetsList}" var="asset" indexVar="index">
                        <div class="slds-grid slds-wrap slds-p-top_xxx-small">
                            <div class="slds-col slds-size_6-of-12 slds-p-left_x-small" style="margin: auto;padding-top: 10px;">
                                
                                
                                <span>
                                    <aura:if isTrue="{!asset.FinServ__AssetsAndLiabilitiesType__c == 'Motor Vehicle' || asset.FinServ__AssetsAndLiabilitiesType__c == 'Superannuation'}">
                                        All {!asset.FinServ__AssetsAndLiabilitiesType__c}
                                        <aura:set attribute="else">
                                            {!asset.FinServ__AssetsAndLiabilitiesType__c}
                                        </aura:set>
                                    </aura:if>
                                </span>
                                
                            </div>
                            <div class="slds-col slds-size_6-of-12">
                                
                                <span class="input-dollar3 edit_inputbox">
                                    <lightning:input aura:id="howMuchFieldcheck" required="true"
                                                     min="0" type="number" 
                                                     class="txt-left no-abbr input-style"  
                                                     step="1" 
                                                     value = "{!asset.Value__c}"
                                                     onchange="{!c.doCalculation}"/>
                                    
                                </span>
                                
                            </div>
                        </div>
                    </aura:iteration>
                </div>
            </aura:set>   
            
        </aura:if>
        <div class="slds-m-vertical_x-large div-color">
            <div class="" title="Total Amount">
                Total Assets: $<lightning:formattedNumber value="{!v.totalAssetsAmount}" maximumFractionDigits="2"/>
            </div>
        </div>
        
    </section>
    <aura:if isTrue="{!v.isOpenAlertModal}">
        <section role="dialog" tabindex="-1" aria-labelledby="modal-heading-01" aria-modal="true" aria-describedby="modal-content-id-1" class="slds-modal slds-fade-in-open">
            <div class="slds-modal__container">
                
                <header class="slds-modal__header">
                    <lightning:buttonIcon iconName="utility:close"
                                          onclick="{! c.closeAlertModel }"
                                          alternativeText="close"
                                          variant="bare-inverse"
                                          class="slds-modal__close"/>
                   
                        <lightning:icon iconName="utility:clear" alternativeText="error !" 
                                        size ="large" variant="error "/>
                        <h2 id="modal-heading-01" class="slds-text-heading_medium slds-hyphenate text-err">Are You Sure?</h2> 
                </header>
                
                <div class="slds-modal__content slds-p-around_medium slds-text-align_center" >
                        Do you really want to delete?
                </div>
                
                <footer class="slds-modal__footer slds-text-align_center modal_footer">
                    <lightning:button variant="neutral" 
                                      label="GO BACK"
                                      title="GO BACK"
                                      onclick="{! c.closeAlertModel }"/>
                    <lightning:button variant="brand" 
                                      label="DELETE"
                                      title="DELETE"
                                      onclick="{! c.deleteAssetLiability }"/> 
                </footer>
            </div>
        </section>
        <div class="slds-backdrop slds-backdrop_open"></div>
    </aura:if>
</aura:component>