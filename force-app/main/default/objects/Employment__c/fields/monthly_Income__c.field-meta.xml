<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>monthly_Income__c</fullName>
    <externalId>false</externalId>
    <formula>IF(ISPICKVAL(Income_Frequency__c,&quot;Weekly&quot;), AnnualIncome__c *52/12,
IF(ISPICKVAL(Income_Frequency__c,&quot;Fortnightly&quot;), AnnualIncome__c *26/12,
IF(ISPICKVAL(Income_Frequency__c,&quot;Monthly&quot;), AnnualIncome__c,
IF(ISPICKVAL(Income_Frequency__c,&quot;Annually&quot;), AnnualIncome__c/12,
AnnualIncome__c))))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>monthly Income</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
