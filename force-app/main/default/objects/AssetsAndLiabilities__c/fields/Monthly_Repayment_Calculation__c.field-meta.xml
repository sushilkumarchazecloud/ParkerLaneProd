<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Monthly_Repayment_Calculation__c</fullName>
    <externalId>false</externalId>
    <formula>(Monthly_Repayment__c + Pre_tax_lease_Payment__c + Post_tax_lease_Payment__c) * IF(ISPICKVAL(Repayment_Frequency__c, &quot;Weekly&quot;) , 52, IF(ISPICKVAL(Repayment_Frequency__c, &quot;Fortnightly&quot;), 26, IF(ISPICKVAL(Repayment_Frequency__c, &quot;Monthly&quot;), 12, 1))) / 12</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Monthly Repayment Calculation</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
